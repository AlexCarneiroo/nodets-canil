{{>partias/header}}


{{#banner}}
<section class="banner" style="background-image: url('./pastor-alemao.jpg')">{{banner.title}}</section>
<h2>{{banner.title}} disponíveis para adoção</h2>
{{/banner}}


<div class="container list">
    {{#list}}
        <div class="item">
            <img src="./pastor-alemao.jpg" class="item--image" />
            <div class="item--name">{{name}}</div>
            <div class="item--color">{{color}}</div>
            <div class="item--genre">{{sex}}</div>
        </div>
    {{/list}}

</div>

{{^list}}

    <div class="container no-list">
        <h2>não a animais disponiveis nessas condiçoes</h2>
    </div>


{{/list}}

{{>partias/footer}}